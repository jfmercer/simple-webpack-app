{
  "name": "simple-webpack-app",
  "version": "0.1.0",
  "description": "An example of a simple webpack application",
  "scripts": {
    "build": "rm -rf build/; webpack --config config/webpack.prod.js --production --progress --profile --bail",
    "fix:js": "eslint src/ --fix",
    "lint:js": "eslint src/",
    "start": "webpack-dev-server --progress --config config/webpack.dev.js"
  },
  "main": "src/main.js",
  "repository": {
    "url": "https://github.com/jfmercer/simple-webpack-app.git",
    "type": "git"
  },
  "author": {
    "name": "John F. Mercer",
    "email": "john.f.mercer@gmail.com"
  },
  "engines": {
    "node": ">=6.8.0"
  },
  "license": "GPL-3.0",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.6.0",
    "eslint-loader": "^1.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0",
    "webpack-validator": "^2.2.9"
